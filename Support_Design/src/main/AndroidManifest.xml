<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.support.design">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@style/FloatingActionButton_Demo_Theme"
            android:name="com.design.FloatingActionButton.FloatingMainActivity"
            android:label="@string/floating_action_button" />
        <activity android:name="com.design.FloatingActionButton.CheeseDetailActivity"
            android:theme="@style/FloatingActionButton_Demo_Theme"/>
    </application>

</manifest>